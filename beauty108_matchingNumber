#include <iostream>
#include <string>
using namespace std;

int main()
{
    int n;
    cout<<"enter the length of array"<<endl;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    for (int j = 0; j < n; j++)
    {
        for (int k = j + 1; k < n; k++)
        {
            if (arr[j] == arr[k])
            {
                cout << "minimum index for repeating value" << endl;
                cout << j + 1 << endl;
                return 0;
            }
        }
    }
}
